{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "globals": {
        "process": "readonly",
        "setImmediate": "readonly"
    },
    "rules": {
        "array-callback-return": "error",
        "arrow-spacing": "error",
        "block-spacing": "error",
        "brace-style": ["error", "1tbs", {"allowSingleLine": true}],
        "class-methods-use-this": "warn",
        "comma-dangle": ["error", "always-multiline"],
        "comma-spacing": "error",
        "comma-style": "error",
        "complexity": "error",
        "computed-property-spacing": "error",
        "consistent-this": "error",
        "curly": ["error", "multi-or-nest", "consistent"],
        "dot-location": ["error", "property"],
        "dot-notation": "warn",
        "eol-last": "warn",
        "eqeqeq": ["error", "smart"],
        "func-call-spacing": "warn",
        "func-names": ["warn", "as-needed"],
        "generator-star-spacing": "warn",
        "grouped-accessor-pairs": "warn",
        "implicit-arrow-linebreak": "warn",
        "indent": ["error", "tab", {"SwitchCase": 1}],
        "keyword-spacing": "error",
        "key-spacing": "error",
        "linebreak-style": "error",
        "lines-between-class-members": ["error", "always", {"exceptAfterSingleLine": true}],
        "newline-per-chained-call": "error",
        "new-cap": "error",
        "new-parens": "error",
        "nonblock-statement-body-position": ["error", "below"],
        "no-alert": "error",
        "no-array-constructor": "warn",
        "no-caller": "error",
        "no-div-regex": "warn",
        "no-duplicate-imports": "warn",
        "no-extend-native": "error",
        "no-extra-bind": "warn",
        "no-extra-boolean-cast": "warn",
        "no-eval": "error",
        "no-fallthrough": "off",
        "no-lonely-if": "warn",
        "no-new-func": "warn",
        "no-new-object": "warn",
        "no-new-wrappers": "warn",
        "no-proto": "warn",
        "no-self-compare": "warn",
        "no-template-curly-in-string": "warn",
        "no-throw-literal": "error",
        "no-trailing-spaces": "error",
        "no-unmodified-loop-condition": "error",
        "no-undef-init": "warn",
        "no-unneeded-ternary": "warn",
        "no-unreachable-loop": "warn",
        "no-unused-expressions": ["warn", {"allowShortCircuit": true, "allowTernary": true}],
        "no-unused-labels": "off",
        "no-unused-vars": ["error", {"varsIgnorePattern": "[iI]gnore"}],
        "no-useless-call": "warn",
        "no-useless-computed-key": "warn",
        "no-useless-concat": "warn",
        "no-useless-constructor": "warn",
        "no-useless-rename": "warn",
        "no-useless-return": "warn",
        "no-use-before-define": ["error", "nofunc"],
        "no-var": "warn",
        "no-warning-comments": "error",
        "no-whitespace-before-property": "error",
        "no-with": "error",
        "object-curly-newline": ["error", {"multiline": true}],
        "object-curly-spacing": ["error", "always", {"objectsInObjects": false, "arraysInObjects": false }],
        "one-var": ["error", {"let": "always", "const": "consecutive"}],
        "operator-assignment": "warn",
        "operator-linebreak": ["warn", "after"],
        "prefer-arrow-callback": "warn",
        "prefer-const": ["warn", {"destructuring": "all"}],
        "prefer-numeric-literals": "warn",
        "prefer-object-has-own": "warn",
        "prefer-object-spread": "warn",
        "prefer-promise-reject-errors": "warn",
        "prefer-rest-params": "error",
        "prefer-template": "warn",
        "quotes": ["error", "single"],
        "radix": "warn",
        "require-atomic-updates": "error",
        "require-await": "warn",
        "rest-spread-spacing": "error",
        "semi": "error",
        "semi-spacing": "error",
        "semi-style": "error",
        "spaced-comment": "warn",
        "space-before-blocks": "error",
        "space-before-function-paren": "error",
        "space-infix-ops": "warn",
        "space-in-parens": "error",
        "space-unary-ops": ["warn", {"words": true, "nonwords": false}],
        "strict": "error",
        "switch-colon-spacing": "error",
        "template-curly-spacing": "error",
        "yoda": "error"
    },
    "ignorePatterns": ["*.log"],
    "overrides": [{
        "files": ["**/modules/**/*.js"],
        "globals": {
            "access": "readonly",
            "addConfig": "readonly",
            "discordjs": "readonly",
            "getBot": "readonly",
            "getConfigurable": "readonly",
            "inspect": "readonly",
            "loadNew": "readonly",
            "log": "readonly",
            "modules": "readonly",
            "OwnerID": "readonly",
            "timespan": "readonly",
            "reload": "readonly",
            "Utils": "readonly",
            "_": "readonly"
        },
        "rules": {
            "no-self-assign": "off",
            "no-undef": "warn",
            "no-unused-vars": ["warn", {"args": "none","vars": "local", "varsIgnorePattern": "in(Guild|AllGuilds|AllDM|All)|[iI]gnore"}]
        }
    }, {
        "files": ["**/test/**/*.js"],
        "globals": {
            "after": "readonly",
            "afterEach": "readonly",
            "before": "readonly",
            "beforeEach": "readonly",
            "describe": "readonly",
            "it": "readonly"
        },
        "rules": {
            "func-names": ["off"],
            "prefer-arrow-callback": "off"
        }
    }]
}
